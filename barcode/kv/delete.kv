MDBoxLayout:
	orientation: 'vertical'
	padding: [10,10,10,10]
	spacing: dp(11)
	adaptive_height: True  # This ensures dynamic height adjustment
	# md_bg_color: .4, .3, .9, 1

	

	Image:
		source: "assets/bin.png"
		size_hint: None, None
		size: dp(150), dp(200)
		pos_hint: {'center_x': 0.5, 'center_y': 0.5}
		allow_stretch: True
		keep_ratio: False
			
